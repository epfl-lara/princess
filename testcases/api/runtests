#!/bin/sh

unset CLASSPATH

rm *.class

scalac -language:implicitConversions,postfixOps,reflectiveCalls -cp ../../bin SimpleAPITest.scala
scala -cp ../../bin:. SimpleAPITest

scalac -language:implicitConversions,postfixOps,reflectiveCalls -cp ../../bin SimpleAPITest2.scala
scala -cp ../../bin:. SimpleAPITest2
